<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="budgie-sensibo-monitor">
  <schema id="com.solus-project.budgie-sensibo-monitor">

    <key type="s" name="apikey">
      <default>'YOURAPIKEY'</default>
      <summary>API Key</summary>
      <description>Sensibo API Key</description>
    </key>

    <key type="s" name="deviceid">
      <default>'YOURDEVICEID'</default>
      <summary>Device ID</summary>
      <description>Sendibi Device name ID</description>
    </key>

    <key type="b" name="onoff">
      <default>false</default>
      <summary>On/Off status</summary>
      <description>Sensibo Device On/Off status</description>
    </key>

    <key type="i" name="intervals">
      <default>1</default>
      <summary>Update Interval</summary>
      <description>Sensibo update Intervals(minutes)</description>
    </key>
  </schema>

</schemalist>
